<mui:ModernWindow x:Class="Project.BarApplication.AdministrativeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Project.BarApplication"
        xmlns:customControls="clr-namespace:Project.BarApplication.CustomControls"
        xmlns:mui="http://firstfloorsoftware.com/ModernUI"
        mc:Ignorable="d"
        Title="AdministrativeWindow" Height="600" Width="800"
        ContentSource="/Pages/Login.xaml">
    <Window.Resources> 
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Templates/TabItemResource.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Window.Resources>
    <mui:ModernWindow.TitleLinks>
        <mui:Link x:Name="TitleLinkMain"/>
    </mui:ModernWindow.TitleLinks> 
</mui:ModernWindow>

