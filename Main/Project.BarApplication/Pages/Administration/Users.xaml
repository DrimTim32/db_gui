<Page x:Class="Project.BarApplication.Pages.Users"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Project.BarApplication.Pages"
      xmlns:mui="http://firstfloorsoftware.com/ModernUI"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="Users">
    <Grid Style="{StaticResource ContentRoot}">
        <mui:ModernTab SelectedSource="/Content/Users/AllUsers.xaml" Layout="List">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="All" Source="/Content/Users/AllUsers.xaml" />
                <mui:Link DisplayName="Waiters" Source="/Content/Users/Waiters.xaml" /> 
                <mui:Link DisplayName="Chefs" Source="/Content/Users/Chefs.xaml" /> 
                <mui:Link DisplayName="Add New" Source="/Content/Users/AddUsers.xaml" /> 
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</Page>
